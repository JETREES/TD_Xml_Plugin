    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.td_xml_plugin" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, plugin.build.jar.td_xml_plugin" description="build all modules"/>
  
  <target name="all" depends="build.modules" description="build all"/>
