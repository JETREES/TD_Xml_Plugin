    <patternset refid="excluded.from.module.td_xml_plugin"/>
  </patternset>
  
  <path id="td_xml_plugin.module.sourcepath">
    <dirset dir="${module.td_xml_plugin.basedir}">
      <include name="src"/>
      <include name="resources"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.td_xml_plugin" depends="compile.module.td_xml_plugin.production,compile.module.td_xml_plugin.tests" description="Compile module TD Xml Plugin"/>
  
  <target name="compile.module.td_xml_plugin.production" depends="register.custom.compilers" description="Compile module TD Xml Plugin; production classes">
    <mkdir dir="${td_xml_plugin.output.dir}"/>
    <javac2 destdir="${td_xml_plugin.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.td_xml_plugin}/javac">
      <compilerarg line="${compiler.args.td_xml_plugin}"/>
      <bootclasspath refid="td_xml_plugin.module.bootclasspath"/>
      <classpath refid="td_xml_plugin.module.production.classpath"/>
      <src refid="td_xml_plugin.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.td_xml_plugin"/>
    </javac2>
    
    <copy todir="${td_xml_plugin.output.dir}">
      <fileset dir="${module.td_xml_plugin.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.td_xml_plugin.basedir}/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.td_xml_plugin.tests" depends="register.custom.compilers,compile.module.td_xml_plugin.production" description="compile module TD Xml Plugin; test classes" unless="skip.tests"/>
  
  <target name="clean.module.td_xml_plugin" description="cleanup module">
    <delete dir="${td_xml_plugin.output.dir}"/>
    <delete dir="${td_xml_plugin.testoutput.dir}"/>
  </target>
  
  <property name="td_xml_plugin.plugin.path.jar" value="${module.td_xml_plugin.basedir}/TD Xml Plugin.zip"/>
  
  <!-- Build archive for plugin 'TD Xml Plugin' -->
  <target name="plugin.build.jar.td_xml_plugin" depends="compile.module.td_xml_plugin" description="Build plugin archive for module &#39;TD Xml Plugin&#39;">
    <property name="tmp.dir.td_xml_plugin" value="${module.td_xml_plugin.basedir}/../temp"/>
    <mkdir dir="${tmp.dir.td_xml_plugin}"/>
    <mkdir dir="${tmp.dir.td_xml_plugin}/lib"/>
    <jar destfile="${tmp.dir.td_xml_plugin}/lib/TD Xml Plugin.jar" duplicate="preserve">
      <zipfileset dir="${module.td_xml_plugin.basedir}/out/production/TD Xml Plugin"/>
      <zipfileset file="${module.td_xml_plugin.basedir}/resources/META-INF/plugin.xml" prefix="META-INF"/>
      <manifest>
        <attribute name="Created-By" value="IntelliJ IDEA"/>
        <attribute name="Manifest-Version" value="1.0"/>
      </manifest>
    </jar>
    <copy file="${module.td_xml_plugin.basedir}/../../../../poi-3.17/poi-ooxml-3.17.jar" tofile="${tmp.dir.td_xml_plugin}/lib/poi-ooxml-3.17.jar"/>
    <copy file="${module.td_xml_plugin.basedir}/../../../../poi-3.17/poi-ooxml-schemas-3.17.jar" tofile="${tmp.dir.td_xml_plugin}/lib/poi-ooxml-schemas-3.17.jar"/>
    <copy file="${module.td_xml_plugin.basedir}/../../../../poi-3.17/poi-3.17.jar" tofile="${tmp.dir.td_xml_plugin}/lib/poi-3.17.jar"/>
    <copy file="${module.td_xml_plugin.basedir}/../../../../poi-3.17/ooxml-lib/xmlbeans-2.6.0.jar" tofile="${tmp.dir.td_xml_plugin}/lib/xmlbeans-2.6.0.jar"/>
    <zip destfile="${td_xml_plugin.plugin.path.jar}">
      <fileset dir="${module.td_xml_plugin.basedir}/../temp"/>
    </zip>
    <delete dir="${tmp.dir.td_xml_plugin}"/>
  </target>
  
